FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    leftWall
    {
        type            nutkWallFunction;
        value           $internalField;
    }
    rightWall
    {
        type            nutkWallFunction;
        value           $internalField;
    }
    lowerWall
    {
        type            nutkWallFunction;
        value           $internalField;
    }
    atmosphere
    {
        type            calculated;
        value           $internalField;
    }
    defaultFaces
    {
        type            empty;
    }
}


