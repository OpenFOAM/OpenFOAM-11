FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            totalPressure;
        p0              uniform 10;
    }
    lowerOutlet
    {
        type            entrainmentPressure;
        p0              uniform 0;
    }
    upperOutlet
    {
        type            entrainmentPressure;
        p0              uniform 0;
    }
    wall
    {
        type            fixedFluxPressure;
    }
}

