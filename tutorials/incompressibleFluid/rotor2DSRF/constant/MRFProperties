FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      MRFProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

SRF
{
    select          all;

    origin    (0 0 0);
    axis      (0 0 1);

    rpm       60;
}

