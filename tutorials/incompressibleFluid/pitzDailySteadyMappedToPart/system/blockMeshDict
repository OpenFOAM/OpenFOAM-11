    (0 0 -0.5)
    (0 1 -0.5)
    (2 -1 -0.5)
    (2 0 -0.5)
    (2 1 -0.5)

    (-2 0 0.5)
    (-2 1 0.5)
    (0 -1 0.5)
    (0 0 0.5)
    (0 1 0.5)
    (2 -1 0.5)
    (2 0 0.5)
    (2 1 0.5)
);

blocks
(
    hex (0 3 4 1 8 11 12 9) (30 40 1) simpleGrading (0.2 5 1)
    hex (2 5 6 3 10 13 14 11) (30 40 1) simpleGrading (5 0.2 1)
    hex (3 6 7 4 11 14 15 12) (30 40 1) simpleGrading (5 5 1)
);

defaultPatch
{
    name frontAndBack;
    type empty;
}

