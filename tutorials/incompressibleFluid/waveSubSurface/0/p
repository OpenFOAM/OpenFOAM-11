FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type            fixedFluxPressure;
    }
    "(top|inlet)"
    {
        type            fixedFluxExtrapolatedPressure;
    }
    outlet
    {
        type            totalPressure;
        p0              $internalField;
        value           $internalField;
    }
    "(front|back)"
    {
        type            symmetryPlane;
    }
}

// ************************************************************************* //
