FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

porosity
{
    type            explicitPorositySource;

    explicitPorositySourceCoeffs
    {
        cellZone        porosity;

        type            fixedCoeff;

        fixedCoeffCoeffs
        {
            alpha       (500 -1000 -1000);
            beta        (0 0 0);
            rhoRef      1;

            coordinateSystem
            {
                type    cartesian;
                origin  (0 0 0);
                coordinateRotation
                {
                    type    axesRotation;
                    e1      (0.70710678 0.70710678 0);
                    e2      (0 0 1);
                }
            }
        }
    }
}


