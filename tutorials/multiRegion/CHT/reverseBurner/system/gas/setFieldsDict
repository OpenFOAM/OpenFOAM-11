FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue N2 0.79
    volScalarFieldValue O2 0.21
    volScalarFieldValue CH4 0
);

regions
(
    boxToCell
    {
        box     (0 0 -0.01) (0.01 0.1 0.01);
        fieldValues
        (
            volScalarFieldValue N2 0
            volScalarFieldValue O2 0
            volScalarFieldValue CH4 1
        );
    }
);


