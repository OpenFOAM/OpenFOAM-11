FoamFile
{
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 350;

boundaryField
{
    wall_inner
    {
        type            coupledTemperature;
        Tnbr            T.liquid;
        value           $internalField;
    }
    wall_outer
    {
        type            externalTemperature;
        q               uniform 66919.2;
        value           $internalField;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}

