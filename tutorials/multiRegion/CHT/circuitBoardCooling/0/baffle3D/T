FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(floor|baffle3D0|baffle3D1|baffle3D_side)"
    {
        type            zeroGradient;
    }

    "baffle3D_to_fluid_.*"
    {
        type            coupledTemperature;
        value           $internalField;
    }
}


