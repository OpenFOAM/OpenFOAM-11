FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      T.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 297;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 297;
    }
    outlet
    {
        type            calculated;
        value           uniform 297;
    }
    top
    {
        type            calculated;
        value           uniform 297;
    }
    bottom
    {
        type            calculated;
        value           uniform 297;
    }
    frontAndBack
    {
        type            empty;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 297;
    }
}


