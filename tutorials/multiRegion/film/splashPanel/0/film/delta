FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/film";
    object      delta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0.00013;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    surface
    {
        type            zeroGradient;
    }

    wall
    {
        type            filmContactAngle;

        contactAngle
        {
            type            constant;
            theta0          70;
        }

        value           $internalField;
    }
}


