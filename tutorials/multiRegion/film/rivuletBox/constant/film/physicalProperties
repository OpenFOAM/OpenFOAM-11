thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    properties      liquid;
    energy          sensibleInternalEnergy;
}

mixture
{
    H2O;
}

sigma
{
    type    constant;
    sigma   0.07;
}

deltaWet    1e-8;


