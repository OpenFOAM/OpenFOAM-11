FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

filmCloudTransfer
{
    type    filmCloudTransfer;

    ejection
    {
        model   dripping;

        deltaStable 0;

        minParticlesPerParcel 100;

        parcelDistribution
        {
            type            RosinRammler;
            Q               0;
            min             5e-04;
            max             0.0012;
            d               7.5e-05;
            n               0.5;
        }
    }
}

