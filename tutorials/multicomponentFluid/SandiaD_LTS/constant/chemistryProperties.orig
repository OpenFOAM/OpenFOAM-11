#includeEtc "caseDicts/solvers/chemistry/TDAC/chemistryPropertiesFlame.cfg"

chemistryType
{
    solver            ode;
}

chemistry       on;

initialChemicalTimeStep 1e-7;

odeCoeffs
{
    solver          seulex;
    absTol          1e-8;
    relTol          1e-1;
}

tabulation
{
    // Scale factors used in the definition of the ellipsoid of accuracy
    scaleFactor
    {
        Temperature  1000;
    }
}

