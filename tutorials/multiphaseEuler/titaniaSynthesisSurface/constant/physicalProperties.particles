FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         multicomponentMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

species (TiO2 TiO2_s);

defaultSpecie TiO2;

TiO2
{
    specie
    {
        molWeight   79.87880;
    }
    equationOfState
    {
        rho         4230;
    }
    thermodynamics
    {
        Hf          -1.18307e+07;
        Cp          684.246;
    }
    transport
    {
        mu          1e-7;
        Pr          8.0e-6;
    }
}

TiO2_s
{
    $TiO2
}


