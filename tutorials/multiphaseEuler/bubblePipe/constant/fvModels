    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

massSource
{
    type            massSource;

    cellZone        injection;

    massFlowRate    6e-7;

    phase           air1;
    rho             rho.air1;

    fieldValues
    {
        f1.air1     0;
        f2.air1     0;
        f3.air1     1;
        f4.air1     0;
        f5.air1     0;
        U.air1      (0 0 0);
    }
}

