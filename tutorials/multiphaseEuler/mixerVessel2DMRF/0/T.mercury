FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T.mercury;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0];

internalField   uniform 300;

boundaryField
{
    rotor
    {
        type            zeroGradient;
        value           uniform 300;
    }

    stator
    {
        type            zeroGradient;
        value           uniform 300;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}


