FoamFile
{
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight   18.015;
    }
    thermodynamics
    {
        Cp          1006.3;
        Hf          6e3;
    }
    transport
    {
        mu          9.86e-06;
        Pr          0.71;
    }
}


