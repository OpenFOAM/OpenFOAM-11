FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.5;

boundaryField
{
    outer
    {
        type            uniformInletOutlet;
        uniformInletValue 1.5;
        value           uniform 1.5;
    }

    ground
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }

    blockedFaces
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }

    baffleWall
    {
        type            kqRWallFunction;
        value           uniform 1.5;
    }

    baffleCyclic_half0
    {
        type            cyclic;
    }

    baffleCyclic_half1
    {
        type            cyclic;
    }
}


