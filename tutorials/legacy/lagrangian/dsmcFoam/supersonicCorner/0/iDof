FoamFile
{
    format      ascii;
    class       volScalarField;
    object      iDof;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            zeroGradient;
    }

    "entrance.*"
    {
        type            symmetryPlane;
    }

    walls
    {
        type            calculated;
        value           uniform 0;
    }

}

