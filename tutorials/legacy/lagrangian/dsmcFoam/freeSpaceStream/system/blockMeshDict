FoamFile
{
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;

vertices
(
    (-5 -4 -4)
    (5 -4 -4)
    (5 4 -4)
    (-5 4 -4)
    (-5 -4 4)
    (5 -4 4)
    (5 4 4)
    (-5 4 4)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 16 16) simpleGrading (1 1 1)
);

boundary
(
    outlet
    {
        type patch;
        faces
        (
            (1 2 6 5)
        );
    }

    inlet
    {
        type patch;
        faces
        (
            (0 4 7 3)
        );
    }

    sides
    {
        type patch;
        faces
        (
            (2 3 7 6)
            (0 1 5 4)
            (4 5 6 7)
            (0 3 2 1)
        );
    }
);


