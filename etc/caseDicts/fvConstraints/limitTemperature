FoamFile
{
    format      ascii;
    class       dictionary;
    object      limitTemperature;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type       limitTemperature;

select        all;

min        <200>;
max        <6000>;

