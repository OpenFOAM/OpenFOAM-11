FoamFile
{
    format      ascii;
    class       dictionary;
    object      pdfDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Number of intervals/bins in pdf plot
nIntervals      20;

// Number of samples
nSamples        10000;

// Type of pdf
type            RosinRammler;

// Write data flag
writeData       true;

// PDF model coefficients
RosinRammlerDistribution
{
    minValue        1e-06;
    maxValue        200e-06;
    d               60.0e-06;
    n               0.8;
}

