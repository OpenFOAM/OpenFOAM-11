FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      momentumTransport;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType laminar;

laminar
{
    model           generalisedNewtonian;

    viscosityModel  CrossPowerLaw;

    CrossPowerLawCoeffs
    {
        nu0         [0 2 -1 0 0 0 0]  0.01;
        nuInf       [0 2 -1 0 0 0 0]  10;
        m           [0 0 1 0 0 0 0]   0.4;
        n           [0 0 0 0 0 0 0]   3;
    }
}

