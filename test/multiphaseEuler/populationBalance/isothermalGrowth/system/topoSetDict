FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    injection;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (0 0.04 0) (0.1 0.05 0.1);
    }

    {
        name    injection;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     injection;
    }
);

